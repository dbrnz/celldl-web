<cell-diagram xmlns="http://www.cellml.org/celldl/1.0#">

    <bond-graph>
        <potential id="u1"/>
        <potential id="u2"/>
        <flow id="v1">
            <component input="u1" output="u2" class="potassium"/>
        </flow>
    </bond-graph>

    <style>
        cell-diagram {
            width: 500;            /* SVG `pixel` units */
            height: 500;           /* SVG `pixel` units */
        }

        #u1 { position: 20%, 45%; }
        #v1 { position: 15% right #u1; }
        #u2 { position: 15% right #v1; }
    </style>


</cell-diagram>
